<h2><h2><span class="page_label">User Listing</span></h2>
<div class = "about">
<table>
  <tr>
    <th>Login</th>
    <th>Email</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.login %></td>
    <td><%= user.email %></td>
    <td><%= link_to 'Profile', user_path( user ) %></td>
  </tr>
  <% end %>
</table>

<%= will_paginate @users %>

</div>