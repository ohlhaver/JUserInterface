<h1>Edit Topic</h1>

<% form_for @topic_preference, :url => user_topic_preference_path( @user, @topic_preference ) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <%= f.submit 'Update' %> &nbsp;  <%= submit_to_remote 'search_btn', 'Search', :url => story_search_path( :topic ), :update => 'results' %>
<% end %>

<div id="results">
</div>